<%= form_with model: project do |form| %>

  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
  <%= form.label :body %><br>
    <%= form.text_area :body %><br>
  </div>
  <div>
    <%= form.label "Main Image" %>
    <%= form.file_field :main %>
  </div>

  <div>
    <%= form.label "Supporting Images" %>
    <%= form.file_field :supimages, multiple: true %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>